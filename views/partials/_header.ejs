<header>
    <nav class="navbar navbar-expand-md navbar-dark " style="background-color: #9f9cee;">
    <a class="navbar-brand" href="/urls">TinyApp</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup"
      aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
      <div class="navbar-nav">
        <a class="nav-item nav-link" href="/urls">My URLs</a>
        <a class="nav-item nav-link" href="/urls/new">Create New URL</a>
        <a class="nav-item nav-link" href="/login">Login</a>
        <a class="nav-item nav-link" href="/register">Register</a>
      </div>
    </div>
    <%  if(loggedUser) {%>
      <form class="form-inline" action="/logout" method="POST">
        <div style=" font-size: 15px; color: #F5EBFF;">
          Logged in as: <%= users[loggedUser].email %> !   
        </div> 
        <button type="submit" style="background-color: rgb(41, 41, 41); color:#FF80BF" class="btn btn-sm">Logout</button>
      </form>
      <% } %>
  
  </nav>
</header>